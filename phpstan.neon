parameters:
    bootstrapFiles:
        - vendor/autoload.php
        - src/resource/ConstantPhpStan.php
    paths:
        - conf
        - src
        - tests
    ignoreErrors:
        -
           message: "#Callable \\(Closure\\(mixed\\)#"
           path: src/DependencyInjection/StrfTime.php
    checkMissingIterableValueType: false
    checkGenericClassInNonGenericObjectType: false
    phpVersion: 80013
